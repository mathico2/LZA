{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:${PARTITION}:s3:::indigo-palo-bootstrapper"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject"
            ],
            "Resource": [
                "arn:${PARTITION}:s3:::indigo-palo-bootstrapper/*"
            ]
        }
    ]
}